{
  "name": "react-native-fab-elements",
  "version": "0.1.2",
  "description": "FabHotels RN Components",
  "main": "src/index",
  "react-native": "src/index",
  "source": "src/index",
  "types": "types/index.d.ts",
  "files": [
    "src",
    "types",
    "react-native.config.js"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "prerelease": "tsc",
    "release": "release-it",
    "setup": "npm i && cd example && npm i && npx pod-install --quiet",
    "start": "cd example && npm start",
    "ios": "cd example && npm run ios",
    "android": "cd example && npm run android"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/manudeepgodara/react-native-fab-elements",
  "author": "manudeep <manudeep.godara@fabhotels.com> (https://github.com/manudeepgodara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manudeepgodara/react-native-fab-elements/issues"
  },
  "homepage": "https://github.com/manudeepgodara/react-native-fab-elements#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "intl": "^1.2.5",
    "moment": "2.29.1",
    "react-native-loading-placeholder": "0.0.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "release-it": "^14.2.2",
    "typescript": "4.1.3",
    "react-native-vector-icons": "8.1.0",
    "@react-native-community/blur": "3.6.0",
    "react-native-gesture-handler": "1.9.0",
    "react-native-linear-gradient": "2.5.6",
    "react-native-reanimated": "1.13.3",
    "react-native-webview": "11.4.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vector-icons": "8.1.0",
    "@react-native-community/blur": "3.6.0",
    "react-native-gesture-handler": "1.9.0",
    "react-native-linear-gradient": "2.5.6",
    "react-native-reanimated": "1.13.3",
    "react-native-webview": "11.4.3"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "jsxBracketSameLine": true,
          "bracketSpacing": false,
          "singleQuote": true,
          "trailingComma": "all"
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "jsxBracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
